<block>
  <name>Serial Sink PDU</name>
  <key>lilacsat_serial_sink_pdu</key>
  <category>LilacSat</category>
  <import>import lilacsat</import>
  <make>lilacsat.serial_sink_pdu($path, $baudrate, $debug)</make>
  <param>
    <name>Path</name>
    <key>path</key>
    <value>/dev/ttyUSB0</value>
    <type>string</type>
  </param>
  <param>
    <name>Baud Rate</name>
    <key>baudrate</key>
    <value>9600</value>
    <type>int</type>
  </param>
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>True</value>
    <type>bool</type>
    <option>
	<name>On</name>
	<key>True</key>
    </option>
    <option>
	<name>Off</name>
	<key>False</key>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
</block>

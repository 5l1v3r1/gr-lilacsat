<?xml version="1.0"?>
<block>
  <name>Image decoder</name>
  <key>lilacsat_image_decoder</key>
  <category>[lilacsat]</category>
  <import>import lilacsat</import>
  <make>lilacsat.image_decoder($path)</make>
  <param>
    <name>Path</name>
    <key>path</key>

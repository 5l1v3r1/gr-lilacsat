<block>
  <name>Pipe Sink PDU</name>
  <key>lilacsat_pipe_sink_pdu</key>
  <category>LilacSat</category>
  <import>import lilacsat</import>
  <make>lilacsat.pipe_sink_pdu($label, $path, $permission, $debug)</make>
  <param>
    <name>Label</name>
    <key>label</key>
    <type>string</type>
  </param>
  <param>
    <name>Path</name>
    <key>path</key>
    <type>string</type>
  </param>
  <param>
    <name>Permission</name>
    <key>permission</key>
    <type>int</type>
  </param>
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>True</value>
    <type>bool</type>
    <option>
	<name>On</name>
	<key>True</key>
    </option>
    <option>
	<name>Off</name>
	<key>False</key>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
</block>

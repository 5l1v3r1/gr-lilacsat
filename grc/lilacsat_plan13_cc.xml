<block>
  <name>Plan13 CC</name>
  <key>lilacsat_plan13_cc</key>
  <category>LilacSat</category>
  <import>import lilacsat</import>
  <make>lilacsat.plan13_cc($enable, $tle1, $tle2, $lon, $lat, $height, $fc, $samp_rate, $txrx, $verbose)</make>
  <param>
    <name>Enable</name>
    <key>enable</key>
    <value>True</value>
    <type>bool</type>
    <option>
	<name>On</name>
	<key>True</key>
    </option>
    <option>
	<name>Off</name>
	<key>False</key>
    </option>
  </param>
  <param>
    <name>TLE (Line 1)</name>
    <key>tle1</key>
    <type>string</type>
  </param>
  <param>
    <name>TLE (Line 2)</name>
    <key>tle2</key>
    <type>string</type>
  </param>
  <param>
    <name>Longtitude</name>
    <key>lon</key>
    <type>float</type>
  </param>
  <param>
    <name>Latitude</name>
    <key>lat</key>
    <type>float</type>
  </param>
  <param>
    <name>Height (m)</name>
    <key>height</key>
    <type>float</type>
  </param>
  <param>
    <name>Center Frequency (Hz)</name>
    <key>fc</key>
    <type>float</type>
  </param>
  <param>
    <name>Sample Rate (Hz)</name>
    <key>samp_rate</key>
    <type>float</type>
  </param>
  <param>
    <name>TXRX</name>
    <key>txrx</key>
    <value>False</value>
    <type>bool</type>
    <option>
	<name>TX</name>
	<key>True</key>
    </option>
    <option>
	<name>RX</name>
	<key>False</key>
    </option>
  </param>
  <param>
    <name>Verbose</name>
    <key>verbose</key>
    <value>False</value>
    <type>bool</type>
    <option>
	<name>On</name>
	<key>True</key>
    </option>
    <option>
	<name>Off</name>
	<key>False</key>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
  <source>
    <name>freq</name>
    <type>message</type>
    <optional>1</optional>
  </source>
  <source>
    <name>rotator</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
